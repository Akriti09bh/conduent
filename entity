package com.example.jpa.ms_cssp.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

import org.springframework.stereotype.Component;

@Entity
@Table(name = "txn_log")
public class Txn_log {
	
	  @Id
	  @GeneratedValue(strategy = GenerationType.SEQUENCE, generator = "txn_log_generator")
	  private long id;
	  @Column(name = "screen_Name")
	  private String screen_Name;
	  @Column(name = "action_Name")
	  private String action_Name;
	  @Column(name = "service_Name")
	  private String service_Name;
	  @Column(name = "error_Stack_Trace")
	  private String error_Stack_Trace;
	  @Column(name = "layer")
	  private String layer;
	  @Column(name = "apim")
	  private String apim;
	  @Column(name = "type")
	  private String type;
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public String getScreen_Name() {
		return screen_Name;
	}
	public void setScreen_Name(String screen_Name) {
		this.screen_Name = screen_Name;
	}
	public String getAction_Name() {
		return action_Name;
	}
	public void setAction_Name(String action_Name) {
		this.action_Name = action_Name;
	}
	public String getService_Name() {
		return service_Name;
	}
	public void setService_Name(String service_Name) {
		this.service_Name = service_Name;
	}
	public String getError_Stack_Trace() {
		return error_Stack_Trace;
	}
	public void setError_Stack_Trace(String error_Stack_Trace) {
		this.error_Stack_Trace = error_Stack_Trace;
	}
	public String getLayer() {
		return layer;
	}
	public void setLayer(String layer) {
		this.layer = layer;
	}
	public String getApim() {
		return apim;
	}
	public void setApim(String apim) {
		this.apim = apim;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public Txn_log(long id, String screen_Name, String action_Name, String service_Name, String error_Stack_Trace,
			String layer, String apim, String type) {
		super();
		this.id = id;
		this.screen_Name = screen_Name;
		this.action_Name = action_Name;
		this.service_Name = service_Name;
		this.error_Stack_Trace = error_Stack_Trace;
		this.layer = layer;
		this.apim = apim;
		this.type = type;
	}
	@Override
	public String toString() {
		return "Txn_log [id=" + id + ", screen_Name=" + screen_Name + ", action_Name=" + action_Name + ", service_Name="
				+ service_Name + ", error_Stack_Trace=" + error_Stack_Trace + ", layer=" + layer + ", apim=" + apim
				+ ", type=" + type + "]";
	}
	
	
	
}
